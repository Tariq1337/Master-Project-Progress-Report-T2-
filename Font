let font,
  fontsize = 40;

function preload() {
  font = loadFont('Antonio.ttf');
}

function setup() {
  createCanvas(300, 500);

  // Set text characteristics
  textFont(font);
  textSize(fontsize);
  textAlign(CENTER, CENTER);
}

function draw() {
  background(0);
  textAlign(CENTER);
  drawWords(width * 0.5);
  
}

  
function drawWords(x) {
  fill(255);
  text('Trace the Line', x, 80);
}
